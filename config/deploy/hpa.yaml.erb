---
apiVersion: autoscaling/v1
kind: HorizontalPodAutoscaler
metadata:
  name: app
  namespace: maps
spec:
  scaleTargetRef:
    apiVersion: apps/v1beta1
    kind: Deployment
    name: app
  minReplicas: !!int <%= hpa_min_replicas %>
  maxReplicas: !!int <%= hpa_max_replicas %>
  targetCPUUtilizationPercentage: 75
